<?xml version="1.0" encoding="UTF-8" ?>
<zeus2005 xmlns="http://tempuri.org/zeus2005.xsd">
  <General>
    <USE_LUA_SCRIPTS>1</USE_LUA_SCRIPTS>
    <WRITE_LOG>0</WRITE_LOG>
    <PENALTY_FIGHT>1</PENALTY_FIGHT>
  </General>
  <CPlayBook>
    <DEMO_MODE>1</DEMO_MODE>
    <TEST_SKILL>2</TEST_SKILL>
  </CPlayBook>
  <Defence>
    <DEFENCE_DEBUG_MODE>1</DEFENCE_DEBUG_MODE>
    <ATTACK_DEF_MODE>1</ATTACK_DEF_MODE>
    <GOALIE_EVALUATE>0</GOALIE_EVALUATE>
    <DENY_BALL_MODE>1</DENY_BALL_MODE>
    <BACK_LINE_MODE>1</BACK_LINE_MODE>
    <SPECIAL_AREA_BACK_LINE_MODE>0</SPECIAL_AREA_BACK_LINE_MODE>

    <!-- 设置边后卫是否会盯人，1是会，0是不会 -->
    <SIDEBACK_MARKING_MODE>1</SIDEBACK_MARKING_MODE>
    
    <!-- 0是友好模式，1是普通模式，2是暴力模式 -->
    <MARKING_MODE>0</MARKING_MODE>

    <!-- 守门员上前补位模式 0为不补位 -->
    <GOALIE_FILL_IN_MODE>0</GOALIE_FILL_IN_MODE>
  </Defence>
  <CMatchState>
    <BAYESLIST>BayesParams1</BAYESLIST>
    <BAYESPARAM>Skuba_Attack</BAYESPARAM>
  </CMatchState>
  <JAM_AND_PUSH>
    <USE_OPP_DIR>1</USE_OPP_DIR>
    <USE_DRIBBLE_TURN>0</USE_DRIBBLE_TURN>
    <USE_DRIBBLE_KICK>1</USE_DRIBBLE_KICK>
    <USE_INTERCEPT>1</USE_INTERCEPT>
    <USE_ROTATE>0</USE_ROTATE>
    <WEAK_OPP>1</WEAK_OPP>
    <GENTLE_MODE>0</GENTLE_MODE>
    <OPP_HAS_BALL_DIST>50</OPP_HAS_BALL_DIST>
    <CIRCLE_FRONT>1</CIRCLE_FRONT>
    <STATE_PRINT>0</STATE_PRINT>
  </JAM_AND_PUSH>
  <PENALTY_DEFENCE>
    <CATEGORY>3</CATEGORY>
    <FRAME>0</FRAME>
    <AHEAD>20</AHEAD>
  </PENALTY_DEFENCE>
  <CSmartGotoPositionV2>
    <VERBOSE_MODE>0</VERBOSE_MODE>
    <DRAW_TARGET>1</DRAW_TARGET>
    <DO_CHECK>1</DO_CHECK>
    <PATH_PLAN_TYPE>0</PATH_PLAN_TYPE>
  </CSmartGotoPositionV2>
  <CGotoPositionV2>
    <DSS_AVOID>1</DSS_AVOID>

    <!-- 是否对y方向进行补偿 -->
    <USE_COMPENSATION_FOR_Y>1</USE_COMPENSATION_FOR_Y>

    <!-- 守门员单独控制平动参数 -->
    <MAX_TRANSLATION_SPEED_GOALIE>350</MAX_TRANSLATION_SPEED_GOALIE>
    <MAX_TRANSLATION_ACC_GOALIE>1400</MAX_TRANSLATION_ACC_GOALIE>
    <MAX_TRANSLATION_DEC_GOALIE>1400</MAX_TRANSLATION_DEC_GOALIE>

    <!-- 后卫单独控制平动参数 -->
    <MAX_TRANSLATION_SPEED_BACK>300</MAX_TRANSLATION_SPEED_BACK>
    <MAX_TRANSLATION_ACC_BACK>500</MAX_TRANSLATION_ACC_BACK>
    <MAX_TRANSLATION_DEC_BACK>500</MAX_TRANSLATION_DEC_BACK>

    <!-- 其他车的平动参数 -->
    <MAX_TRANSLATION_SPEED>200</MAX_TRANSLATION_SPEED><!-- 300 -->
    <MAX_TRANSLATION_ACC>300</MAX_TRANSLATION_ACC>
    <MAX_TRANSLATION_DEC>500</MAX_TRANSLATION_DEC>

    <MAX_ROTATION_SPEED>15</MAX_ROTATION_SPEED>
    <MAX_ROTATION_ACC>15</MAX_ROTATION_ACC>
    <MAX_ROTATION_DEC>15</MAX_ROTATION_DEC>
    <TRAJECTORY_METHORD>1</TRAJECTORY_METHORD>
    <TRANSLATION_ACC_LIMIT>1400</TRANSLATION_ACC_LIMIT>
  </CGotoPositionV2>
</zeus2005>

